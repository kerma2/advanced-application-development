{
  "compilerOptions": {
    "baseUrl": "src",
    "module": "commonjs",
    "target": "ES6",
    "sourceMap": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./build",
    "types": [
      "node",
    ],
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}